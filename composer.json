{
    "name": "kucera/monolog-extensions-bundle",
    "description": "Bundle adding a set of Monolog extensions.",
    "type": "library",
    "keywords": ["Symfony", "Monolog", "Tracy", "log", "logging", "blue screen"],
    "homepage": "https://github.com/pavelkucera/monolog-extensions-bundle",
    "license": ["MIT", "GPL-2.0", "GPL-3.0"],
    "authors": [
        {
            "name": "Pavel Kučera",
            "homepage": "https://github.com/pavelkucera"
        }
    ],
    "support": {
        "issues": "https://github.com/pavelkucera/monolog-extensions-bundle/issues"
    },
    "require": {
        "php": ">=5.3.2",
        "symfony/monolog-bundle": "~2.6",
        "symfony/dependency-injection": "~2.6",
        "symfony/config": "~2.6",
        "symfony/yaml": "~2.6",
        "kucera/monolog-extensions": "~0.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1"
    },
    "autoload": {
        "psr-4": {
            "Kucera\\MonologExtensionsBundle\\": "./"
        }
    }
}
